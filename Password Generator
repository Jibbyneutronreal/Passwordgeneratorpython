import random

def NumberGen():

    SequenceLength = int(input("How long do you want your sequence to be? "))

    SequenceList = []

    for i in range(SequenceLength):
        makesequence = random.randrange(1, 10, 1)
        SequenceList.append(makesequence)

    SequenceStrList = ''.join(map(str, SequenceList))
    print(SequenceStrList)

NumberGen()
